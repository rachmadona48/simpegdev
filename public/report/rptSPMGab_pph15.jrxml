	<queryString>
		<![CDATA[select
NALOKL AS NALOKL,B.NAMA NAMASPM,A.SPMU AS SPMU,KLOGAD,THBL,A.NAMA,NIP18 As NIP,A.KINERJA,
a.NRK,GOL,NJTUNDA,NPOTABSEN,NJTNDTAMBAH,NBPU,NJTNDSELISIH,NTKD,NJTNDSELBEL,NPPHTUNDA
,NTKDSTLPPH,NASURANSI,NJTUNDABERSIH,E.NPWP,
$P{pBulannama}  AS BULAN ,
SUBSTR(THBL,1,4) AS TAHUN,
(CASE SUBSTR(GOL,2,1)
          WHEN '1' THEN 'I'
          WHEN '2' THEN 'II'
          WHEN '3' THEN 'III'
          WHEN '4' THEN 'IV'
 END ) AS GOLONGAN,
(CASE SUBSTR(GOL,3,1)
          WHEN '1' THEN 'A'
          WHEN '2' THEN 'B'
          WHEN '3' THEN 'C'
          WHEN '4' THEN 'D'
		  WHEN '5' THEN 'E'
 END ) AS PANGKAT,
 nvl(NPPHTUNDA,0) AS SELPPH
from PERS_DUK_PANGKAT_HIST_PPH A INNER JOIN pers_tabel_spmu B ON A.SPMU = B.KODE_SPM
INNER JOIN pers_lokasi_tbl C ON KLOGAD = C.KOLOK
INNER JOIN PERS_ESELON_TBL D ON NVL(A.ESELON,'  ') = D.ESELON
LEFT JOIN  NPWP E ON A.NRK = E.NRK
WHERE
thbl=$P{pTHBL}  and NVL(NPPHTUNDA,0)>0 and a.upload IN (9,1)
order by spmu,klogad asc,stapeg desc,D.CETAKAN, gol desc,a.KODIKF, nrk asc]]>
	</queryString>
