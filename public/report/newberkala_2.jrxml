	<queryString>
		<![CDATA[SELECT
	ROWNUM AS rn,
	nrk,
	nip18,
	nama,
	noskgol,
	TO_CHAR (talhir, 'dd-mm-yyyy') talhir,
	TO_CHAR (mugad, 'dd-mm-yyyy') mugad,
CONCAT(
CONCAT(
substr(TO_CHAR (mugad, 'dd-mm-yyyy'),1,2),
CASE substr(TO_CHAR (mugad, 'dd-mm-yyyy'),4,2)
WHEN '01' THEN ' JANUARI '
WHEN '02' THEN ' FEBRUARI '
WHEN '03' THEN ' MARET '
WHEN '04' THEN ' APRIL '
WHEN '05' THEN ' MEI '
WHEN '06' THEN ' JUNI '
WHEN '07' THEN ' JULI '
WHEN '08' THEN ' AGUSTUS '
WHEN '09' THEN ' SEPTEMBER '
WHEN '10' THEN ' OKTOBER '
WHEN '11' THEN ' NOVEMBER '
WHEN '12' THEN ' DESEMBER '
END),substr(TO_CHAR (mugad, 'dd-mm-yyyy'),7,4)) AS MUGAD_INDO,
	TO_CHAR (tgijazah, 'dd-mm-yyyy') next_brkala,

CONCAT(
CONCAT(
substr(TO_CHAR (tgijazah, 'dd-mm-yyyy'),1,2),
CASE substr(TO_CHAR (tgijazah, 'dd-mm-yyyy'),4,2)
WHEN '01' THEN ' JANUARI '
WHEN '02' THEN ' FEBRUARI '
WHEN '03' THEN ' MARET '
WHEN '04' THEN ' APRIL '
WHEN '05' THEN ' MEI '
WHEN '06' THEN ' JUNI '
WHEN '07' THEN ' JULI '
WHEN '08' THEN ' AGUSTUS '
WHEN '09' THEN ' SEPTEMBER '
WHEN '10' THEN ' OKTOBER '
WHEN '11' THEN ' NOVEMBER '
WHEN '12' THEN ' DESEMBER '
END),substr(TO_CHAR (tgijazah, 'dd-mm-yyyy'),7,4)) AS NEXT_INDO,
	pathir,
	kodik,
	alamat || rt || rw || prop kolok,
	tunjab gjlama,
	tunfung gjbaru,
	masker,
	stapeg,
	mskgol,
	namisu AS kantor,
	titel AS hub_dinas
FROM
	pers_duk_pangkat_bkala
WHERE
	THBL = $P{THBL}
ORDER BY
	noskgol ASC]]>
	</queryString>
